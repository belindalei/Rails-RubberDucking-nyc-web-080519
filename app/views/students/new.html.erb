<h1>New Student Form:</h1>

<% if flash[:messages]%>
    <% flash[:messages].each do |message|%>
      <li><%= message %></li>
    <%end%>
<%end%>

<%= form_for @student do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :mod %>
  <%= f.number_field :mod%>
  <%= f.submit %>
<% end %>

